<Application x:Class="RemoveDislike.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Src/Views/MainWindow.xaml">
    <Application.Resources>

        <ResourceDictionary>

            <FontFamily x:Key="FontAwesome">Resources/Fonts/#FontAwesome</FontFamily>
            <FontFamily x:Key="FontAwesome5">Resources/Fonts/#Font Awesome 5 Free Regular</FontFamily>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Panuon.UI.Silver;component/Control.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>