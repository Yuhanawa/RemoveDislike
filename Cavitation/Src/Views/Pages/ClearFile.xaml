<Page x:Class="Cavitation.Views.Pages.ClearFile"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:Cavitation.Views.Pages"
      xmlns:models="clr-namespace:Cavitation.Views.Models"
      mc:Ignorable="d"
      Title="ClearFile" Height="450" Width="800">
    
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition />
        </Grid.RowDefinitions>
        
        <TextBlock Grid.Row="0"/>
        <Button Grid.Row="0" Content="Go!"/>
        
        <StackPanel Grid.Row="1" Name="RuleList" Initialized="List_OnInitialized"/>
        
        
    </Grid>
    
</Page>
